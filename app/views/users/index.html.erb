<%= link_to 'Events', root_path %>
<%= link_to 'View Attended Events', show_user_path(current_user.username) %>

<h3>Attending</h3>
<% @users.each do |user| %>
  <% if user.attendings.any? %>
    <% user.attendings.each do |attending| %>
      <%= attending.attendee.username %>
    <% end %>
  <% end %>
<% end %>

<% if user_signed_in? %>
<%= button_to 'Sign Out', destroy_user_session_path, method: :delete %>
<% end %>